            },
            "type": "@devlikeapro/n8n-nodes-waha.WAHA",
            "typeVersion": 202502,
            "position": [
                650,
                300
            ],
            "name": "WA: Send Resi",
            "credentials": {
                "wahaApi": {
                    "id": "PIacUGMEHqjAT46z"
                }
            }
        }
    ],
    "connections": {
        "Webhook: Shipping Trigger": {
            "main": [
                [
                    {
                        "node": "Create Biteship Order",
